-- Imports--
local TrollwareLib = loadstring(game:HttpGet("https://solarishub.dev/SolarisLib.lua"))()
local AkaliNotif = loadstring(game:HttpGet(
    "https://raw.githubusercontent.com/Kinlei/Dynissimo/main/Scripts/AkaliNotif.lua"))();
local Notify = AkaliNotif.Notify;
getgenv().ScaryName = false;
TrollwareLib:Notification("Trollware", "Trollware on Top!")
local Trollware = TrollwareLib:New({
    Name = "Trollware",
    FolderToSave = "Trollware"
})

-- Gui Building--
local Outfits = Trollware:Tab("Outfits")
