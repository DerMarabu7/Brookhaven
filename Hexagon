local Extras = Trollware:Tab("Extras")
local OutfitsSection = Outfits:Section("Outfits")
local HeadsSection = Heads:Section("Heads")
local PlayerSection = Player:Section("Player")
local MiscSection = Misc:Section("Misc")
local ExtrasSection = Extras:Section("Extras")
