--Player--
local toggle = PlayerSection:Toggle("ScaryName", false,"Toggle", function(state)
    if state then
        getgenv().ScaryName = true;
    else
        getgenv().ScaryName = false;
    end
while getgenv().ScaryName == true do
wait(0.5)
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName", "I SEE YOU")
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayBio", "YOU CANT HIDE")
wait(1)
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName", "DONT RUN LITTLE ONE")
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayBio", "#####################################################")
wait(1)
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName", "YOURE TASTY")
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayBio", "#####################################################")
wait(1)
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName", "COME HERE")
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayBio", "#####################################################")
end
end)
