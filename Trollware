Heckerman = {
    SchemeColor = Color3.fromRGB(0, 77, 0),
    Background = Color3.fromRGB(0, 0, 0),
    Header = Color3.fromRGB(0, 0, 0),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}
local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/Dynissimo/main/Scripts/AkaliNotif.lua"))();
local Notify = AkaliNotif.Notify;
local TargetName = "";
getgenv().ScaryName = false;
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Trollware", Heckerman)
local Outfits = Window:NewTab("Outfits")
local OutfitsSec = Outfits:NewSection("Outfits")
local Heads = Window:NewTab("Heads")
local HeadsSec = Heads:NewSection("Heads")
local Player = Window:NewTab("Player")
local PlayerSec = Player:NewSection("Player")
local Misc = Window:NewTab("Misc")
local MiscSec = Misc:NewSection("Misc")
local Extras = Window:NewTab("Extras")
local ExtrasSec = Extras:NewSection("Extras")

OutfitsSec:NewButton("Reset Outfit", "Resets you to your Roblox Outfit", function()
wait(0.5)
game:GetService("ReplicatedStorage").RemoteEvents.A11vatarOriginalCharacterC11lient:FireServer("OCA")
Notify({
    Description = "Succesfully changed to normal roblox outfit";
    Title = "Trollware";
    Duration = 3;
    });
end)

